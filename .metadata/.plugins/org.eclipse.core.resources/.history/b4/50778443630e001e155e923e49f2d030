package org.sumitexam.app.SumitFinalExam.services;

import org.sumit.spring.boot.competeapp.dto.MobilePhoneDTO;

import java.util.List;

public interface MobilePhoneService {
    boolean createMobilePhone(MobilePhoneDTO mobilePhoneDTO);
    List<MobilePhoneDTO> getAllMobiles();
    MobilePhoneDTO getMobileDetail(Long mobileId);
    MobilePhoneDTO updateMobileDetail(Long mobileId, MobilePhoneDTO mobilePhoneDTO);
}
